{
    "name": "ec-cube/ec-cube2",
    "description": "EC-CUBE 2.x",
    "type": "project",
    "homepage": "http://www.ec-cube.net/",
    "license": [
        "GPL-2.0",
        "Commercial"
    ],
    "support": {
        "issues": "https://github.com/EC-CUBE/ec-cube2/issues"
    },
    "minimum-stability": "stable",
    "conflict": {
        "symfony/symfony": "*",
        "easycorp/easy-log-handler": "1.0.4|1.0.5",
        "doctrine/inflector": "^1.4"
    },
    "config": {
        "vendor-dir": "data/vendor",
        "platform": {
            "php": "7.4.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "composer/*": true,
            "kylekatarnls/update-helper": true,
            "symfony/flex": true,
            "ocramius/package-versions": true
        }
    },
    "require-dev": {
        "doctrine/instantiator": "~1.5.0",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.9",
        "nanasess/ec-cube2-class-extends-stubs": "^1.0",
        "nanasess/eccube2-fixture-generator": "^2.0",
        "phpdocumentor/reflection-docblock": "~5.5.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "symfony/yaml": "^5.4 || ^6.4",
        "bheller/images-generator": "^1.0",
        "captbaritone/mailcatcher-codeception-module": "^2.4",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.3",
